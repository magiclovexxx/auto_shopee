{
   "author": "ToanNguyen",
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_popup": "popup/popup.html",
      "default_title": "AutoShopee"
   },
   "content_scripts": [ {
      "js": [ "content/shopee/script.js" ],
      "matches": [ "https://shopee.vn/*", "https://banhang.shopee.vn/*" ]
   } ],
   "description": "Giải pháp cho nhà bán hàng Shopee",
   "homepage_url": "https://autoshopee.com",
   "icons": {
      "128": "icons/icon_128.png",
      "48": "icons/icon_48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "AutoShopee",
   "omnibox": {
      "keyword": "autoshopee"
   },
   "options_ui": {
      "chrome_style": true,
      "page": "options/options.html"
   },
   "permissions": [ "*://*.autoshopee.com/*", "*://*.shopee.vn/*", "cookies", "tabs", "runtime", "browserAction" ],
   "short_name": "AutoShopee",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.7.5"
}
